#import "NSArray+All.h"

@implementation NSArray(All)

<% types[:all].each do |target| -%>
- (BOOL) all<%= target.friendly_name %>s:(<%= target.friendly_name %>Predicate)predicate {
  for (<%= target.declare_boxed("element") %> in self) {
    if (!predicate(<%= target.unbox("element") %>)) {
      return NO;
    }
  }

  return YES;
}

<% end -%>
@end