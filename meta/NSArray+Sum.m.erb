#import "NSArray+Sum.h"

@implementation NSArray(Sum)

<% types[:numeric].each do |target| -%>
- (<%= target.name %>) sum<%= target.friendly_name %>s {
  <%= target.declare("sum") %> = 0;

  for (<%= target.declare_boxed("element") %> in self) {
    sum += <%= target.unbox("element") %>;
  }

  return sum;
}

<% end -%>
@end