#import "NSArray+Any.h"

@implementation NSArray(Any)

<% types[:all].each do |target| -%>
- (BOOL) any<%= target.friendly_name %>:(<%= target.friendly_name %>Predicate)predicate {
  for (<%= target.declare_boxed("element") %> in self) {
    if (predicate(<%= target.unbox("element") %>)) {
      return YES;
    }
  }

  return NO;
}

<% end -%>
@end