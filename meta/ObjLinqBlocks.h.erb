<% types[:all].each do |source| -%>
<% types[:all].each do |target| -%>
typedef <%= target.declare %>(^<%= source.friendly_name %>To<%=target.friendly_name %>Converter)(<%= source.declare("input") %>);
<% end -%>
<% end -%>

<% types[:all].each do |target| -%>
typedef BOOL (^<%= target.friendly_name %>Predicate)(<%= target.declare("input") %>);
<% end -%>

<% types[:numeric].each do |target| -%>
typedef <%= target.declare %>(^<%= target.friendly_name %>Addition)(<%= target.declare("x") %>, <%= target.declare("y") %>);
<% end -%>